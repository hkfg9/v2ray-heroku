{
  "name": "Heroku-v2ray",
  "description": "将v2ray部署在Heroku上",
  "keywords": [
    "v2ray VMess VLESS"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议VMess或VLESS，请填小写的vmess或vless",
      "value": "vmess",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/hkfg9/v2ray-heroku",
  "repository": "https://github.com/hkfg9/v2ray-heroku",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
